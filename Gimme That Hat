local function ClonedService(name)
	local Service = (game.GetService)
	local Reference = (cloneref) or function(reference)
		return reference
	end;
	return Reference(Service(game, name))
end;
for i, v in pairs(ClonedService("Players").LocalPlayer.Character:GetDescendants()) do
	if v:IsA("Accessory") then
		print("What accessories you have on")
		print(v.Name)
	end
end;
task.wait()
local plr = ClonedService("Players").LocalPlayer;
local function protectUI(sGui)
	if sGui:IsA("ScreenGui") then
		sGui.ZIndexBehavior = Enum.ZIndexBehavior.Global;
		sGui.DisplayOrder = 999999999;
		sGui.ResetOnSpawn = false;
		sGui.IgnoreGuiInset = true
	end;
	local cGUI = ClonedService("CoreGui")
	local lPlr = ClonedService("Players").LocalPlayer;
	local function NAProtection(inst, var)
		if inst then
			if var then
				inst[var] = "\0"
				inst.Archivable = false
			else
				inst.Name = "\0"
				inst.Archivable = false
			end
		end
	end;
	if gethui then
		NAProtection(sGui)
		sGui.Parent = gethui()
		return sGui
	elseif cGUI and cGUI:FindFirstChild("RobloxGui") then
		NAProtection(sGui)
		sGui.Parent = cGUI:FindFirstChild("RobloxGui")
		return sGui
	elseif cGUI then
		NAProtection(sGui)
		sGui.Parent = cGUI;
		return sGui
	elseif lPlr and lPlr:FindFirstChildWhichIsA("PlayerGui") then
		NAProtection(sGui)
		sGui.Parent = lPlr:FindFirstChildWhichIsA("PlayerGui")
		sGui.ResetOnSpawn = false;
		return sGui
	else
		return nil
	end
end;
function mOut(txt, typeLol)
	if typeLol == 1 then
		task.spawn(function()
			local m = Instance.new("Message")
			protectUI(m)
			m.Text = txt;
			task.wait(3)
			m:Destroy()
		end)
	elseif typeLol == 2 then
		task.spawn(function()
			local h = Instance.new("Hint")
			protectUI(h)
			h.Text = txt;
			task.wait(3)
			h:Destroy()
		end)
	end
end;
